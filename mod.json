{
	"geode": "4.10.2",
	"early-load": true,
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "_lil2kki.remina",
	"name": "\u0000",
	"version": "v0.0.0-alpha.1",
	"developers": [ "\u0000" ],
	"description": "remina.bccst.ru",
	"links": {
		"source": "https://github.com/lil2kki/REMINA",
		"community": "https://discord.gg/NXbbv2HZGg",
		"homepage": "https://remina.bccst.ru"
	},
	"dependencies": {
		"geode.node-ids": {
			"version": ">=v1.20.0",
			"importance": "suggested"
		},
		"prevter.imageplus": {
			"version": ">=v1.0.3",
			"importance": "suggested"
		},
		"alphalaneous.happy_textures": {
			"version": ">=v2.0.7",
			"importance": "suggested"
		}
	},
	"resources": {
		"sprites": [
			"assets/sprites/**/**/*.png"
		],
		"spritesheets": {
			"spritesheet": [
				"assets/frames/**/**/*.png"
			]
		}
	},
	"api": {
		"include": [
			"resources/**/**/*.*"
		]
	},
	"settings": {
		"redir request urls": {
			"name": "redir request urls",
			"description": "replace boomlings.com/database with the our server url in all requests",
			"type": "bool",
			"default": true
		},
		"add button sounds": {
			"name": "add button sounds",
			"description": "add sounds to buttons that don't have them",
			"type": "bool",
			"default": true
		}
	}
}
